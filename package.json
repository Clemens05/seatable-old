{
  "name": "savvytec-seatable",
  "version": "1.4.0",
  "description": "Easy-to-use SeaTable API wrapper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "_format_old": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "_old_files": [
    "lib/**/*"
  ],
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clemens05/seatable-old.git"
  },
  "keywords": [
    "SeaTable",
    "API"
  ],
  "author": "savvytec",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Clemens05/seatable/issues"
  },
  "homepage": "https://github.com/Clemens05/seatable#readme",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "zod": "^3.21.4"
  }
}
